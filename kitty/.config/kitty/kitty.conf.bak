# BEGIN_KITTY_THEME
# Tomorrow Night
include current-theme.conf
# END_KITTY_THEME

font_size 11.5

text_composition_strategy legacy

# BEGIN_KITTY_FONTS
font_family      family='MesloLGS Nerd Font' style=Regular
bold_font        family='MesloLGS Nerd Font' postscript_name=MesloLGSNF-Bold
italic_font      family='MesloLGS Nerd Font' postscript_name=MesloLGSNF-Italic
bold_italic_font family='MesloLGS Nerd Font' postscript_name=MesloLGSNF-BoldItalic
# END_KITTY_FONTS

# Kitty close confirmation
confirm_os_window_close 0

